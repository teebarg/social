{
    "version": 2,
    "buildCommand": "pip install -r requirements.txt",
    "devCommand": "fastapi run app/main.py --host 0.0.0.0 --port $PORT",
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}
