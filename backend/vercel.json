{
    "version": 2,
    "buildCommand": "poetry install",
    "devCommand": "fastapi run app/main.py --host 0.0.0.0 --port $PORT",
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ]
  }